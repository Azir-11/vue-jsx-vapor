{
  "version": "2.0.1",
  "packageManager": "pnpm@10.4.1",
  "description": "Vue JSX Vapor",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "scripts": {
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "build": "pnpm run --filter=\"./packages/*\" build",
    "typecheck": "tsmc --noEmit",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "test": "vitest",
    "release": "bumpp -r --all -x 'pnpm run changelog'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.8.1",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.28.1",
    "@sxzz/eslint-config": "^6.0.2",
    "@ts-macro/tsc": "^0.1.24",
    "@types/node": "^22.13.10",
    "@vue-macros/reactivity-transform": "3.0.0-beta.4",
    "bumpp": "^10.0.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unplugin-raw": "^0.4.4",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "estree-walker": "2.0.2"
    }
  }
}
