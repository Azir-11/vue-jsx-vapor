{
  "version": "2.1.6",
  "packageManager": "pnpm@10.4.1",
  "description": "Vue JSX Vapor",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue-jsx",
    "volar",
    "vapor",
    "babel",
    "compiler"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "scripts": {
    "dev": "pnpm run --filter=\"./packages/*\" --parallel dev",
    "build": "pnpm run --filter=\"./packages/*\" build",
    "typecheck": "tsmc --noEmit",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "test": "vitest",
    "release": "pnpm lint && bumpp -r --all -x 'pnpm run changelog'",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "docs:dev": "pnpm run -C ./docs dev",
    "docs:preview": "pnpm run -C ./docs preview",
    "docs:build": "pnpm run -C ./docs build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^6.0.2",
    "@ts-macro/tsc": "^0.1.24",
    "@types/node": "^22.13.10",
    "@vue-macros/reactivity-transform": "catalog:",
    "bumpp": "^10.0.3",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.22.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unplugin-raw": "^0.4.4",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "estree-walker": "2.0.2"
    }
  }
}
