{
  "compilerOptions": {
    "target": "es2017",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxImportSource": "vue",
    "lib": ["esnext", "DOM"],
    "useDefineForClassFields": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["dist", "eslint.config.js", "playground"]
}
