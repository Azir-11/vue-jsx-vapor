{
  "name": "vue-jsx-vapor",
  "version": "2.1.5",
  "packageManager": "pnpm@10.4.1",
  "description": "Convert Vue JSX to Vapor",
  "type": "module",
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform",
    "vue-jsx",
    "volar",
    "vapor"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vuejs/vue-jsx-vapor#readme",
  "bugs": {
    "url": "https://github.com/vuejs/vue-jsx-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs/vue-jsx-vapor.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./astro": {
      "dev": "./src/astro.ts",
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.js"
    },
    "./rspack": {
      "dev": "./src/rspack.ts",
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./vite": {
      "dev": "./src/vite.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "dev": "./src/webpack.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "dev": "./src/rollup.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "dev": "./src/esbuild.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "dev": "./src/nuxt.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./api": {
      "dev": "./src/api.ts",
      "require": "./dist/api.cjs",
      "import": "./dist/api.js"
    },
    "./raw": {
      "dev": "./src/raw.ts",
      "require": "./dist/raw.cjs",
      "import": "./dist/raw.js"
    },
    "./rolldown": {
      "dev": "./src/rolldown.ts",
      "require": "./dist/rolldown.cjs",
      "import": "./dist/rolldown.js"
    },
    "./unplugin": {
      "dev": "./src/unplugin.ts",
      "require": "./dist/unplugin.cjs",
      "import": "./dist/unplugin.js"
    },
    "./jsx-runtime": {
      "dev": "./src/jsx-runtime.ts",
      "require": "./dist/jsx-runtime.cjs",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "dev": "./src/jsx-runtime.ts",
      "require": "./dist/jsx-runtime.cjs",
      "default": "./dist/jsx-runtime.js"
    },
    "./volar": {
      "dev": "./src/volar.ts",
      "require": "./dist/volar.cjs",
      "import": "./dist/volar.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./astro": {
      "require": "./dist/astro.cjs",
      "import": "./dist/astro.js"
    },
    "./rspack": {
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./nuxt": {
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.js"
    },
    "./api": {
      "require": "./dist/api.cjs",
      "import": "./dist/api.js"
    },
    "./rolldown": {
      "require": "./dist/rolldown.cjs",
      "import": "./dist/rolldown.js"
    },
    "./unplugin": {
      "require": "./dist/unplugin.cjs",
      "import": "./dist/unplugin.js"
    },
    "./raw": {
      "require": "./dist/raw.cjs",
      "import": "./dist/raw.js"
    },
    "./jsx-runtime": {
      "require": "./dist/jsx-runtime.cjs",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "require": "./dist/jsx-runtime.cjs",
      "default": "./dist/jsx-runtime.js"
    },
    "./volar": {
      "require": "./dist/volar.cjs",
      "import": "./dist/volar.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsup",
    "dev": "DEV=true tsup",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@babel/core": "catalog:",
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@vue-jsx-vapor/babel": "workspace:*",
    "@vue-jsx-vapor/compiler": "workspace:*",
    "@vue-jsx-vapor/macros": "workspace:*",
    "@vue-macros/volar": "^3.0.0-beta.5",
    "magic-string-stack": "^0.1.2",
    "ts-macro": "catalog:",
    "unplugin": "catalog:",
    "unplugin-utils": "^0.2.4"
  },
  "devDependencies": {
    "@nuxt/kit": "catalog:",
    "@nuxt/schema": "catalog:",
    "@types/babel__core": "catalog:",
    "csstype": "^3.1.3",
    "vue": "catalog:"
  }
}
